    <class>com.ctex.ct.gcompet.modelo.Projetos</class>
    <class>com.ctex.ct.gcompet.modelo.AreasProjetos</class>
    <class>com.ctex.ct.gcompet.modelo.AreasEmpresas</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>

    <class>com.ctex.ct.gcompet.modelo.Capacidades</class>
    <class>com.ctex.ct.gcompet.modelo.CapacidadesAreas</class>
    <class>com.ctex.ct.gcompet.modelo.Empresas</class>
    <class>com.ctex.ct.gcompet.modelo.Perfil</class>
